employee_one = int(input())
employee_two = int(input())
employee_three = int(input())
students_count = int(input())

total_students_per_hour = employee_one + employee_two + employee_three
hours = 0

if total_students_per_hour > 0:
    while students_count > 0:
        hours += 1
        if hours % 4 == 0:
            continue
        students_count -= total_students_per_hour

print(f"Time needed: {hours}h.")
